pmid = {15679680},
title = {Resident doctors' understanding of their roles as clinical teachers},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15679680 http://doi.wiley.com/10.1111/j.1365-2929.2004.02063.x},
