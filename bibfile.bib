issn = {10402446},
month = {aug},
number = {8},
pages = {761--767},
pmid = {18667892},
title = {From the educational bench to the clinical bedside: Translating the Dreyfus developmental model to the learning of clinical skills},
url = {https://insights.ovid.com/crossref?an=00001888-200808000-00017},
volume = {83},
year = {2008}
}
